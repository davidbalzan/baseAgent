{
  "name": "baseagent",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "build": "pnpm -r run build",
    "typecheck": "pnpm -r exec tsc --noEmit",
