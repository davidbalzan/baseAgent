{
  "name": "baseagent",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "build": "pnpm -r run build",
    "typecheck": "pnpm -r exec tsc --noEmit",
    "dev": "pnpm --filter @baseagent/app run dev",
    "clean": "pnpm -r exec rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "vitest": "^3.0.0"
  }
}
