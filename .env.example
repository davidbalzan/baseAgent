# baseAgent — Environment Variables
# Copy this file to .env and fill in your values.

# LLM Providers (at least one required)
OPENROUTER_API_KEY=           # Dev/testing — access 300+ models
ANTHROPIC_API_KEY=            # Production Claude (Opus 4.6 / Sonnet 4.6)
OPENAI_API_KEY=               # Production OpenAI

# Channel Adapters (enable in config/default.yaml)
TELEGRAM_BOT_TOKEN=           # From @BotFather
DISCORD_BOT_TOKEN=            # From Discord Developer Portal

# Web Search
BRAVE_SEARCH_API_KEY=         # For web_search tool — https://brave.com/search/api/

# Server
PORT=3000                     # HTTP server port
